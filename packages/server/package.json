{
  "name": "@sample-monorepo/server",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs",
    "prebuild": "yarn clean",
    "build": "ts-build ./src --cjs",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint -p src",
    "lint:test": "tslint -p test",
    "test": "mocha -r @ts-tools/node \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx",
    "prestart": "yarn workspace @sample-monorepo/app build:umd",
    "start": "nodemon -r @ts-tools/node -r ./src/index.tsx",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@sample-monorepo/app": "^1.0.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "files": [
    "cjs",
    "src"
  ],
  "license": "MIT",
  "repository": "git@github.com:wixplosives/sample-monorepo.git",
  "private": true,
  "publishConfig": {
    "access": "public"
  }
}
