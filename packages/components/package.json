{
  "name": "@sample-monorepo/components",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "prebuild": "yarn clean",
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint -p src",
    "lint:test": "tslint -p test",
    "test": "mocha -r @ts-tools/node \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx",
    "prepack": "yarn build"
  },
  "dependencies": {
    "react": "^16.6.1"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "license": "MIT",
  "repository": "git@github.com:wixplosives/sample-monorepo.git",
  "sideEffects": false,
  "private": true,
  "publishConfig": {
    "access": "public"
  }
}
